<?xml version="1.0" encoding="UTF-8"?>
<project name="module_catchall.catchall-sponge" default="compile.module.catchall.catchall-sponge">
  <dirname property="module.catchall.catchall-sponge.basedir" file="${ant.file.module_catchall.catchall-sponge}"/>
  
  <property name="module.jdk.home.catchall.catchall-sponge" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.catchall.catchall-sponge" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.catchall.catchall-sponge" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.catchall.catchall-sponge" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="catchall.catchall-sponge.output.dir" value="${module.catchall.catchall-sponge.basedir}/../../../out/production/catchall.catchall-sponge"/>
  <property name="catchall.catchall-sponge.testoutput.dir" value="${module.catchall.catchall-sponge.basedir}/../../../out/test/catchall.catchall-sponge"/>
  
  <path id="catchall.catchall-sponge.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="catchall.catchall-sponge.module.production.classpath">
    <path refid="${module.jdk.classpath.catchall.catchall-sponge}"/>
  </path>
  
  <path id="catchall.catchall-sponge.runtime.production.module.classpath">
    <pathelement location="${catchall.catchall-sponge.output.dir}"/>
  </path>
  
  <path id="catchall.catchall-sponge.module.classpath">
    <path refid="${module.jdk.classpath.catchall.catchall-sponge}"/>
    <pathelement location="${catchall.catchall-sponge.output.dir}"/>
  </path>
  
  <path id="catchall.catchall-sponge.runtime.module.classpath">
    <pathelement location="${catchall.catchall-sponge.testoutput.dir}"/>
    <pathelement location="${catchall.catchall-sponge.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.catchall.catchall-sponge">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.catchall.catchall-sponge">
    <patternset refid="excluded.from.module.catchall.catchall-sponge"/>
  </patternset>
  
  
  <target name="compile.module.catchall.catchall-sponge" depends="compile.module.catchall.catchall-sponge.production,compile.module.catchall.catchall-sponge.tests" description="Compile module catchall.catchall-sponge"/>
  
  <target name="compile.module.catchall.catchall-sponge.production" depends="register.custom.compilers" description="Compile module catchall.catchall-sponge; production classes"/>
  
  <target name="compile.module.catchall.catchall-sponge.tests" depends="register.custom.compilers,compile.module.catchall.catchall-sponge.production" description="compile module catchall.catchall-sponge; test classes" unless="skip.tests"/>
  
  <target name="clean.module.catchall.catchall-sponge" description="cleanup module">
    <delete dir="${catchall.catchall-sponge.output.dir}"/>
    <delete dir="${catchall.catchall-sponge.testoutput.dir}"/>
  </target>
</project>