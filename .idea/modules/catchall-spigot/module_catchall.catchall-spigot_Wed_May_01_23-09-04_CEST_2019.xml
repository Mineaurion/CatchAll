<?xml version="1.0" encoding="UTF-8"?>
<project name="module_catchall.catchall-spigot" default="compile.module.catchall.catchall-spigot">
  <dirname property="module.catchall.catchall-spigot.basedir" file="${ant.file.module_catchall.catchall-spigot}"/>
  
  <property name="module.jdk.home.catchall.catchall-spigot" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.catchall.catchall-spigot" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.catchall.catchall-spigot" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.catchall.catchall-spigot" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="catchall.catchall-spigot.output.dir" value="${module.catchall.catchall-spigot.basedir}/../../../out/production/catchall.catchall-spigot"/>
  <property name="catchall.catchall-spigot.testoutput.dir" value="${module.catchall.catchall-spigot.basedir}/../../../out/test/catchall.catchall-spigot"/>
  
  <path id="catchall.catchall-spigot.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="catchall.catchall-spigot.module.production.classpath">
    <path refid="${module.jdk.classpath.catchall.catchall-spigot}"/>
  </path>
  
  <path id="catchall.catchall-spigot.runtime.production.module.classpath">
    <pathelement location="${catchall.catchall-spigot.output.dir}"/>
  </path>
  
  <path id="catchall.catchall-spigot.module.classpath">
    <path refid="${module.jdk.classpath.catchall.catchall-spigot}"/>
    <pathelement location="${catchall.catchall-spigot.output.dir}"/>
  </path>
  
  <path id="catchall.catchall-spigot.runtime.module.classpath">
    <pathelement location="${catchall.catchall-spigot.testoutput.dir}"/>
    <pathelement location="${catchall.catchall-spigot.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.catchall.catchall-spigot">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.catchall.catchall-spigot">
    <patternset refid="excluded.from.module.catchall.catchall-spigot"/>
  </patternset>
  
  
  <target name="compile.module.catchall.catchall-spigot" depends="compile.module.catchall.catchall-spigot.production,compile.module.catchall.catchall-spigot.tests" description="Compile module catchall.catchall-spigot"/>
  
  <target name="compile.module.catchall.catchall-spigot.production" depends="register.custom.compilers" description="Compile module catchall.catchall-spigot; production classes"/>
  
  <target name="compile.module.catchall.catchall-spigot.tests" depends="register.custom.compilers,compile.module.catchall.catchall-spigot.production" description="compile module catchall.catchall-spigot; test classes" unless="skip.tests"/>
  
  <target name="clean.module.catchall.catchall-spigot" description="cleanup module">
    <delete dir="${catchall.catchall-spigot.output.dir}"/>
    <delete dir="${catchall.catchall-spigot.testoutput.dir}"/>
  </target>
</project>