<?xml version="1.0" encoding="UTF-8"?>
<project name="module_catchall.catchall-common" default="compile.module.catchall.catchall-common">
  <dirname property="module.catchall.catchall-common.basedir" file="${ant.file.module_catchall.catchall-common}"/>
  
  <property name="module.jdk.home.catchall.catchall-common" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.catchall.catchall-common" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.catchall.catchall-common" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.catchall.catchall-common" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="catchall.catchall-common.output.dir" value="${module.catchall.catchall-common.basedir}/../../../out/production/catchall.catchall-common"/>
  <property name="catchall.catchall-common.testoutput.dir" value="${module.catchall.catchall-common.basedir}/../../../out/test/catchall.catchall-common"/>
  
  <path id="catchall.catchall-common.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="catchall.catchall-common.module.production.classpath">
    <path refid="${module.jdk.classpath.catchall.catchall-common}"/>
  </path>
  
  <path id="catchall.catchall-common.runtime.production.module.classpath">
    <pathelement location="${catchall.catchall-common.output.dir}"/>
  </path>
  
  <path id="catchall.catchall-common.module.classpath">
    <path refid="${module.jdk.classpath.catchall.catchall-common}"/>
    <pathelement location="${catchall.catchall-common.output.dir}"/>
  </path>
  
  <path id="catchall.catchall-common.runtime.module.classpath">
    <pathelement location="${catchall.catchall-common.testoutput.dir}"/>
    <pathelement location="${catchall.catchall-common.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.catchall.catchall-common">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.catchall.catchall-common">
    <patternset refid="excluded.from.module.catchall.catchall-common"/>
  </patternset>
  
  
  <target name="compile.module.catchall.catchall-common" depends="compile.module.catchall.catchall-common.production,compile.module.catchall.catchall-common.tests" description="Compile module catchall.catchall-common"/>
  
  <target name="compile.module.catchall.catchall-common.production" depends="register.custom.compilers" description="Compile module catchall.catchall-common; production classes"/>
  
  <target name="compile.module.catchall.catchall-common.tests" depends="register.custom.compilers,compile.module.catchall.catchall-common.production" description="compile module catchall.catchall-common; test classes" unless="skip.tests"/>
  
  <target name="clean.module.catchall.catchall-common" description="cleanup module">
    <delete dir="${catchall.catchall-common.output.dir}"/>
    <delete dir="${catchall.catchall-common.testoutput.dir}"/>
  </target>
</project>